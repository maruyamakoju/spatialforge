{
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "title": "Request Rate (req/s)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 0 },
      "targets": [
        {
          "expr": "rate(spatialforge_requests_total[5m])",
          "legendFormat": "{{method}} {{endpoint}} {{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "reqps" }
      }
    },
    {
      "title": "Request Latency (p50 / p95 / p99)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 0 },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, rate(spatialforge_request_duration_seconds_bucket[5m]))",
          "legendFormat": "p50"
        },
        {
          "expr": "histogram_quantile(0.95, rate(spatialforge_request_duration_seconds_bucket[5m]))",
          "legendFormat": "p95"
        },
        {
          "expr": "histogram_quantile(0.99, rate(spatialforge_request_duration_seconds_bucket[5m]))",
          "legendFormat": "p99"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" }
      }
    },
    {
      "title": "Inference Duration by Model",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 8 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(spatialforge_inference_duration_seconds_bucket[5m]))",
          "legendFormat": "{{model}} — {{endpoint}} p95"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "s" }
      }
    },
    {
      "title": "Inference Throughput",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 8 },
      "targets": [
        {
          "expr": "rate(spatialforge_inferences_total[5m])",
          "legendFormat": "{{model}} — {{endpoint}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "ops" }
      }
    },
    {
      "title": "GPU Memory Usage",
      "type": "gauge",
      "gridPos": { "h": 8, "w": 6, "x": 0, "y": 16 },
      "targets": [
        {
          "expr": "spatialforge_gpu_memory_used_bytes / spatialforge_gpu_memory_total_bytes * 100",
          "legendFormat": "VRAM %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "thresholds": {
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 60 },
              { "color": "red", "value": 85 }
            ]
          }
        }
      }
    },
    {
      "title": "GPU Memory (Absolute)",
      "type": "stat",
      "gridPos": { "h": 8, "w": 6, "x": 6, "y": 16 },
      "targets": [
        {
          "expr": "spatialforge_gpu_memory_used_bytes",
          "legendFormat": "Used"
        },
        {
          "expr": "spatialforge_gpu_memory_total_bytes",
          "legendFormat": "Total"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "bytes" }
      }
    },
    {
      "title": "Error Rate",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 6, "x": 12, "y": 16 },
      "targets": [
        {
          "expr": "rate(spatialforge_errors_total[5m])",
          "legendFormat": "{{endpoint}} — {{error_type}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "errors/s" }
      }
    },
    {
      "title": "Active Async Jobs",
      "type": "stat",
      "gridPos": { "h": 8, "w": 6, "x": 18, "y": 16 },
      "targets": [
        {
          "expr": "spatialforge_active_jobs",
          "legendFormat": "{{job_type}}"
        }
      ]
    },
    {
      "title": "API Calls by Plan",
      "type": "piechart",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 24 },
      "targets": [
        {
          "expr": "increase(spatialforge_api_key_calls_total[24h])",
          "legendFormat": "{{plan}}"
        }
      ]
    },
    {
      "title": "Latency by Endpoint",
      "type": "table",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 24 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(spatialforge_request_duration_seconds_bucket[1h]))",
          "legendFormat": "{{endpoint}}",
          "format": "table",
          "instant": true
        }
      ]
    },
    {
      "title": "Rate Limit Rejections",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 32 },
      "targets": [
        {
          "expr": "rate(spatialforge_rate_limit_hits_total[5m])",
          "legendFormat": "{{plan}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "hits/s" }
      }
    },
    {
      "title": "Upload File Sizes (p95)",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 32 },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, rate(spatialforge_upload_size_bytes_bucket[5m]))",
          "legendFormat": "{{endpoint}} p95"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "bytes" }
      }
    }
  ],
  "schemaVersion": 39,
  "style": "dark",
  "tags": ["spatialforge", "gpu", "inference"],
  "templating": { "list": [] },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "SpatialForge — Operations Dashboard",
  "uid": "spatialforge-main",
  "version": 1
}
